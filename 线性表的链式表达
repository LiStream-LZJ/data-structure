#include <stdio.h>
#include <stdlib.h>
typedef int ElemType;
typedef struct LNode
{
	ElemType date;
	struct LNode *next;
}LNode, *LinkList;
int InitList(LinkList &L)
{
	L = new LNode;
	L->next = NULL;
	printf("初始化成功\n");
	return 1;
}
int Input(LinkList &L)
{
	int i, j, x;
	printf("请输入位数:\n");
	scanf("%d", &i);
	for (j = 1; j <= i; j++)
	{
		printf("\nplease input");
		scanf("%d", &x);
		L->date = x;
		L = L->next;
	}
	printf("\nok\n");
}
int main()
{
	LinkList *L;
	InitList(L);
	Input(L);
	return 0;
}
